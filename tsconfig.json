{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "include": [
      "./src/@types/*.d.ts"
    ],
    "paths": {
      "@Graphql/*": ["src/graphql/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Components/*": ["src/components/*"],
      "@Screens/*": ["src/screens/*"],
      "@Stores/*": ["src/stores/*"],
      "@Assets/*": ["assets/*"],
      "@/*": ["src/*"],
      "#/*": ["./*"]
    }
  }
}
