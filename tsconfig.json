{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@Graphql/*": ["src/graphql/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Components/*": ["src/components/*"],
      "@Screens/*": ["src/screens/*"],
      "@Stores/*": ["src/stores/*"],
      "@Assets/*": ["assets/*"],
      "@/*": ["src/*"]
    }
  }
}
